/var/log/lighttpd-access.log
/var/log/lighttpd-error.log
/var/www/var/log/lighttpd-access.log
/var/www/var/log/lighttpd-error.log {
        nodateext
        size 10M
        start 1
        rotate 10
        missingok
        notifempty
        compress
        delaycompress
        copytruncate
}
